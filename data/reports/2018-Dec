            "Acknowledgments": [
                {
                    "Name": [
                        {
                            "Value": "Jaanus Kp Clarified Security working with Trend Micro's Zero Day Initiative"
                        }
                    ],
                    "URL": [
                        "http://www.zerodayinitiative.com/"
