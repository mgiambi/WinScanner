            "CVSSScoreSets": [
                {
                    "BaseScore": 4.2,
                    "ProductID": [
                        "10724-11497"
                    ],
                    "TemporalScore": 3.8,
                    "Vector": "CVSS:3.0/AV:N/AC:H/PR:N/UI:R/S:U/C:L/I:L/A:N/E:P/RL:O/RC:C"
