            "Title": {
                "Value": "Internet Explorer Memory Corruption Vulnerability"
            }
        },
        {
            "Acknowledgments": [
                {
                    "Name": [
                        {
                            "Value": "62600BCA031B9EB5CB4A74ADDDD6771E working with Trend Micro\u2019s Zero Day Initiative (ZDI)"
                        }
